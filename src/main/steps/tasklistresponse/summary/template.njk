{% extends "common/respondent-summary/template.njk" %}
{% block page_content %}
{% if section %}
    <h1 class="govuk-heading-xl govuk-!-margin-bottom-50px">{{ section }}</h1>
  {% endif %}
      <div class="govuk-warning-text">
        <span class="govuk-warning-text__icon" aria-hidden="true">!</span>
        <strong class="govuk-warning-text__text">
            <span class="govuk-warning-text__assistive">{{warning1}}</span>
            {{yourResponse}}
        </strong>

    </div>
  {% if title %}
    <p class="govuk-body-l">{{ title }}</p>
  {% endif %}
 
{% endblock %}