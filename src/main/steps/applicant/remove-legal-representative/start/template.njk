{% extends "common/template.njk" %}

{% block page_content %}
  <div class="govuk-form-group">
    <h1 class="govuk-heading-xl">
    </h1>
    <div class=" govuk-!-margin-bottom-3">
      {% if section %}
        <span class="govuk-heading-l">{{ section }}</span>
      {% endif %}
      <div class="govuk-form-group">
        <fieldset class="govuk-fieldset">
        <p class="govuk-body-l">#{{userCase.id}}</p>
        </fieldset>
        <hr class="govuk-section-break govuk-section-break govuk-section-break--visible">
      </div>
      <div class="govuk-!-margin-bottom-4">
        
        <p class="govuk-body">{{removeLegalRepresentativeInformationLine1}}</p>

        <p class="govuk-body">{{removeLegalRepresentativeInformationLine2}}</p>

        <p class="govuk-body">{{removeLegalRepresentativeInformationLine3}}</p>

        <p class="govuk-body">{{removelLegalRepresentativeInformationLine4}}</p>

      </div>

    </div>
  </div>
{% endblock %}